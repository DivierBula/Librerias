                    <div class="title-col">Observaciones</div>
                    <div class="table-content mini-tabla nopadding">
                        <table class="flex-table table table-striped table-hover">
                            <thead>
                                <tr>
                                    <th>Fecha Cambio</th>
                                    <th>Fecha Respuesta</th>
                                    <th>Observaciones</th>
                                </tr>
                            </thead>
                            <tbody class="scroll">
                                <tr *ngFor="let det of model.varBitacora;">
                                    <td>{{det.fechaCambio | date: "dd-MM-yyyy"}}</td>
                                    <td>{{det.fechaRespuesta | date: "dd-MM-yyyy"}}</td>
                                    <td>{{det.observaciones}}</td>
                                </tr>
                                <tr *ngIf="model.varBitacora?.length == 0">
                                    <div class="dbempty">Sin datos</div>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
        <div class="btn-modal">
            <div class="btn-col pointer noselect modal-action" (click)="closeBitacoraModal(false)">Cerrar <div
                    class="inner-btn">
                    <i class="fas fa-times-circle"></i></div>
            </div>
        </div>
    </div>
</app-modal>

